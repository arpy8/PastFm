{
  "name": "PastFm",
  "version": "1.0.0",
  "description": "Fetches last played songs from YT Music",
  "manifest_version": 3,
  "author": "Arpit Sengar (arpy8)",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://arpy8-pastfm-backend.hf.space/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "PastFm"
  }
}